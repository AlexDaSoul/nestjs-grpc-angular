{
    "name": "auth",
    "version": "1.0.0",
    "description": "Auth service",
    "author": "@AlexDaSoul",
    "repository": {
        "type": "git",
        "url": "https://github.com/AlexDaSoul/nestjs-grpc-angular"
    },
    "license": "ISC",
    "scripts": {
        "docker:debug": "ts-node-dev --inspect=0.0.0.0 --respawn --transpileOnly -r tsconfig-paths/register main.ts",
        "migrate:generate": "ts-node  -r tsconfig-paths/register ./node_modules/typeorm/cli migration:generate -d services/dal/db/migrations -f lib/dal/migrations/migrations.config.ts -n",
        "migrate:create": "ts-node  -r tsconfig-paths/register ./node_modules/typeorm/cli migration:create -d services/dal/db/migrations -f lib/dal/migrations/migrations.config.ts -n",
        "migrate:revert": "ts-node  -r tsconfig-paths/register services/dal/db/migrations-common/migrations.config.ts"
    },
    "dependencies": {
        "@grpc/proto-loader": "^0.5.3",
        "@nestjs/common": "^6.10.1",
        "@nestjs/core": "^6.10.1",
        "@nestjs/microservices": "^6.10.1",
        "@nestjs/typeorm": "^6.2.0",
        "class-transformer": "^0.2.3",
        "class-validator": "^0.11.0",
        "google-protobuf": "^3.10.0",
        "grpc": "^1.24.2",
        "jsonwebtoken": "^8.5.1",
        "pem": "^1.14.3",
        "pg": "^7.12.1",
        "reflect-metadata": "^0.1.13",
        "rxjs": "^6.5.3",
        "typeorm": "^0.2.20"
    },
    "devDependencies": {
        "@types/bcrypt": "^3.0.0",
        "@types/jsonwebtoken": "^8.3.5",
        "@types/node": "^12.12.8",
        "@types/pem": "^1.9.5",
        "ts-node": "^8.5.2",
        "ts-node-dev": "^1.0.0-pre.44",
        "tsconfig-paths": "^3.9.0",
        "typescript": "^3.7.2"
    }
}
