{
    "compilerOptions": {
        "module": "commonjs",
        "noImplicitAny": false,
        "removeComments": true,
        "noLib": false,
        "allowSyntheticDefaultImports": true,
        "emitDecoratorMetadata": true,
        "preserveSymlinks": true,
        "target": "esnext",
        "sourceMap": true,
        "noUnusedLocals": true,
        "noUnusedParameters": false,
        "newLine": "LF",
        "experimentalDecorators": true,
        "allowJs": true,
        "declaration": false,
        "baseUrl": "."
    }
}
